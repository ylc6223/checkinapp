<view class="zsy_calendar"><view class="calendar_info"><text class="title">每日记录</text><text class="desc">{{'('+($root.m0===$root.m1?'':$root.m2+'年')+$root.m3+'月)'}}</text><text data-event-opts="{{[['tap',[['goToDate']]]]}}" hidden="{{!(showBackToTodayBtn)}}" class="backToToday" style="{{'color:'+(dateActiveColor)+';'}}" bindtap="__e">回到今天</text></view><view class="calendar_week"><block wx:for="{{week}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="calendar_week__item">{{item}}</view></block></view><view class="calendar_swiper"><block wx:if="{{swiperMode==='open'}}"><swiper style="{{'height:'+($root.m4)+';'}}" circular="{{true}}" current="{{current}}" duration="{{duration}}" skip-hidden-item-layout="{{true}}" data-event-opts="{{[['change',[['e0',['$event']]]]]}}" bindchange="__e"><block wx:for="{{3}}" wx:for-item="swiper" wx:for-index="swiperIndex" wx:key="swiperIndex"><swiper-item class="swiper-item"><date-box vue-id="{{'e929b3bc-1-'+swiperIndex}}" dates="{{calendarSwiperDates[swiperIndex]}}" cellHeight="{{cellHeight}}" dateActiveColor="{{dateActiveColor}}" swiperMode="{{swiperMode}}" data-event-opts="{{[['^chooseDate',[['chooseDate']]]]}}" bind:chooseDate="__e" bind:__l="__l"></date-box></swiper-item></block></swiper></block><block wx:else><swiper style="{{'height:'+($root.m5)+';'}}" circular="{{true}}" current="{{shrinkCurrent}}" duration="{{duration}}" skip-hidden-item-layout="{{true}}" data-event-opts="{{[['change',[['e1',['$event']]]]]}}" bindchange="__e"><block wx:for="{{3}}" wx:for-item="swiper" wx:for-index="swiperIndex" wx:key="swiperIndex"><swiper-item class="swiper-item"><date-box vue-id="{{'e929b3bc-2-'+swiperIndex}}" dates="{{calendarSwiperShrinkDates[swiperIndex]}}" cellHeight="{{cellHeight}}" dateActiveColor="{{dateActiveColor}}" swiperMode="{{swiperMode}}" data-event-opts="{{[['^chooseDate',[['chooseShrinkDate']]]]}}" bind:chooseDate="__e" bind:__l="__l"></date-box></swiper-item></block></swiper></block></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="calendar_toggle" bindtap="__e"><view class="{{['icon',(swiperMode==='close')?'down':'']}}"></view></view></view>